{
  "name": "picture-conversation",
  "version": "0.0.1",
  "repository": "git@github.com:KyMback/picture-conversation.git",
  "author": "KyMback",
  "license": "MIT",
  "private": false,
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack serve -c ./configs/webpack.config.dev.js --open",
    "build": "cross-env NODE_ENV=production webpack -c ./configs/webpack.config.prod.js",
    "predeploy": "yarn build",
    "deploy": "gh-pages -d dist",
    "bundle-analyze": "cross-env ANALYZE=true yarn build",
    "test": "jest",
    "lint": "eslint . --cache --ext .js,.jsx,.ts,.tsx",
    "prepare": "husky install",
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint --cache --fix"
  },
  "dependencies": {
    "buffer": "^6.0.3",
    "js-file-download": "^0.4.12",
    "mobx": "^6.3.5",
    "mobx-react-lite": "^3.2.1",
    "pngjs": "^6.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@babel/preset-env": "^7.15.8",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.15.0",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.1",
    "@types/jest": "^27.0.2",
    "@types/pngjs": "^6.0.1",
    "@types/react": "^17.0.30",
    "@types/react-dom": "^17.0.9",
    "@typescript-eslint/eslint-plugin": "^5.2.0",
    "@typescript-eslint/parser": "^5.2.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-module-resolver": "^4.1.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.1.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.1",
    "gh-pages": "^3.2.3",
    "html-webpack-plugin": "^5.4.0",
    "husky": "^7.0.4",
    "jest": "^27.3.1",
    "lint-staged": "^11.2.4",
    "prettier": "^2.4.1",
    "react-refresh": "^0.10.0",
    "typescript": "^4.4.4",
    "webpack": "^5.58.2",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.3.1",
    "webpack-merge": "^5.8.0"
  }
}
